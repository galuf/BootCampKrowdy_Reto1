{ 
  "manifest_version": 3,
  "name": "Bootcamp Information from linkedin", 
  "version": "1.0", 
  "description": "Get Information from contact profiles",
  "content_scripts":[
    {
      "matches":["https://www.linkedin.com/*"],
      "js": ["./app.js"]
    }
  ],
  "action": { 
    "default_popup": "App/index.html", 
    "default_icon": {
      "32": "./Icons/32.png",
      "48": "./Icons/48.png"
    }
  }, 
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "32": "./Icons/32.png",
    "48": "./Icons/48.png"
  }
} 